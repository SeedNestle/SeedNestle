{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/seed-nestle" }
    },
    {
      "src": "backend/src/app.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/src/app.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
