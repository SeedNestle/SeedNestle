{
  "version": 2,
  "builds": [
    {
      "src": "dist/seed-nestle/browser/**",
      "use": "@vercel/static"
    },
    {
      "src": "backend/dist/api/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/dist/api/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/seed-nestle/browser/index.html"
    }
  ]
}
