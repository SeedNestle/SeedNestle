{
  "version": 2,
  "builds": [
    {
      "src": "dist/seed-nestle/**",
      "use": "@vercel/static-build"
    },
    {
      "src": "backend/dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/seed-nestle/index.html"
    }
  ]
}
